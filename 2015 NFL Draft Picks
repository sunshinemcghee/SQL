/* Put your data in here and query it! */

/*
1st 3 rounds of the NFL 2015 Draft
Collected by: https://www.khanacademy.org/profile/BobbyandKaren/
*/

CREATE TABLE picks(
	id INTEGER PRIMARY KEY,
	pick_number INTEGER,
	name TEXT,
	college TEXT,
	picked_by TEXT,
	position TEXT);

