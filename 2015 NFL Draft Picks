/* Select all data in table. */
SELECT * 
FROM picks;

/* How many draft picks did each team make? */
SELECT picked_by, COUNT(picked_by) 
AS picks_by_team 
FROM picks 
GROUP BY picked_by;


/* How many players were drafted from each position? */
SELECT position, COUNT(position) 
AS picks_by_position 
FROM picks 
GROUP BY position;


/* How many players were drafted from each represented college? */
SELECT college, COUNT(college) 
AS picks_by_college 
FROM picks 
GROUP BY college;



/* How many players from each position were drafted from each represented college? Group the data by largest to smallest draft class per position from each college. */
SELECT position, college, COUNT(*) 
as num_players
FROM picks
GROUP BY position, college
HAVING 
  COUNT(*) >= 
    (SELECT MAX(num_players) 
     FROM 
       (SELECT 
          position, 
          college, 
          COUNT(*) AS num_players
        FROM 
          picks
             GROUP BY 
          position, 
          college) AS count_table 
     WHERE count_table.position = picks.position)
ORDER BY 
  position, 
  num_players DESC;
  
  
  
  /* How many players from each position were drafted to each NFL team? Group the data by largest to smallest draft class per position to each NFL team. */
SELECT position, picked_by, COUNT(*) 
as num_players
FROM picks
GROUP BY position, picked_by
HAVING 
  COUNT(*) >= 
    (SELECT MAX(num_players) 
     FROM 
       (SELECT 
          position, 
          picked_by, 
          COUNT(*) AS num_players
        FROM 
          picks
             GROUP BY 
          position, 
          picked_by) AS count_table 
     WHERE count_table.position = picks.position)
ORDER BY 
  position, 
  num_players DESC;


  /* Which QB's were drafted in 2015? Where did they get drafted, and where did they go to college? What number pick were they in the draft? */
SELECT *
FROM picks
WHERE position = "QB";


  /* Which position were the top 5 most common drafted the most in the 2015 draft? */
SELECT position, count(*)
AS num_players
FROM picks
GROUP BY position
ORDER BY num_players DESC
LIMIT 5;


  /* How many players from each position were drafted in the 2015 NFL draft? */
SELECT position, count(*)
AS num_players
FROM picks
GROUP BY position
ORDER BY num_players DESC;


  /* Find the earliest drafted pick from each position. */
SELECT position, MIN(pick_number)
AS earliest_pick
FROM picks
GROUP BY position
ORDER BY earliest_pick;

/* Find the earliest drafted player from each position. */
SELECT name, position, MIN(pick_number)
AS earliest_pick
FROM picks
GROUP BY position
ORDER BY earliest_pick;


/* Find the earliest drafted player from each represented college. */
SELECT name, position, college, MIN(pick_number)
AS earliest_pick
FROM picks
GROUP BY college
ORDER BY earliest_pick;


/* Find each NFL team's first draft pick for the 2015 draft. */
SELECT name, position, picked_by, MIN(pick_number)
AS NFL_first_pick
FROM picks
GROUP BY picked_by
ORDER BY NFL_first_pick;
